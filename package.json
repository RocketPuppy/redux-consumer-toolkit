{
  "name": "redux-reducer-toolkit",
  "version": "1.0.0",
  "description": "Toolkit for combining and creating efficient derived reducers",
  "main": "dist/bundle.js",
  "repository": {
      "type": "github",
      "url": "https://github.com/RocketPuppy/redux-reducer-toolkit"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "babel src/ -d build/",
